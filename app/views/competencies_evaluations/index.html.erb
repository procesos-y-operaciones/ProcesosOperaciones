<p id="notice"><%= notice %></p>

<h1>Competencies Evaluations</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @competencies_evaluations.each do |competencies_evaluation| %>
      <tr>
        <td><%= link_to 'Show', competencies_evaluation %></td>
        <td><%= link_to 'Edit', edit_competencies_evaluation_path(competencies_evaluation) %></td>
        <td><%= link_to 'Destroy', competencies_evaluation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Competencies Evaluation', new_competencies_evaluation_path %>
