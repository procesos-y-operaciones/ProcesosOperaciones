<p id="notice"><%= notice %></p>

<h1>Goal Types</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @goal_types.each do |goal_type| %>
      <tr>
        <td><%= link_to 'Show', goal_type %></td>
        <td><%= link_to 'Edit', edit_goal_type_path(goal_type) %></td>
        <td><%= link_to 'Destroy', goal_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Goal Type', new_goal_type_path %>
