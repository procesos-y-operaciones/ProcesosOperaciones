<p id="notice"><%= notice %></p>

<h1>Goals Users</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @goals_users.each do |goals_user| %>
      <tr>
        <td><%= link_to 'Show', goals_user %></td>
        <td><%= link_to 'Edit', edit_goals_user_path(goals_user) %></td>
        <td><%= link_to 'Destroy', goals_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Goals User', new_goals_user_path %>
