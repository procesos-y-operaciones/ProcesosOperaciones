<p id="notice"><%= notice %></p>

<h1>Phases Users</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @phases_users.each do |phases_user| %>
      <tr>
        <td><%= link_to 'Show', phases_user %></td>
        <td><%= link_to 'Edit', edit_phases_user_path(phases_user) %></td>
        <td><%= link_to 'Destroy', phases_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

